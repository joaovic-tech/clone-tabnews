{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "> Implementação do https://www.tabnews.com.br que fiz no https://curso.dev",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migrate:create": "node-pg-migrate -m infra/migrations create",
    "migrate:up": "node-pg-migrate -m infra/migrations up --envPath .env.development"
  },
  "author": "joaovic-tech",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.3.1",
    "next": "^14.2.35",
    "node-pg-migrate": "7.6.1",
    "pg": "8.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "prettier": "3.3.3"
  }
}